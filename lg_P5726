#include <stdio.h>

int main()
{
  int max,min,num,a;
  float sum=0,ans;
  scanf("%d",&num);
  scanf("%d",&a);
  max = min = a;
  sum = sum + max;
  for (int i=1;i<num;i++)
  {
    scanf("%d",&a);
    sum = sum + a;
    if (max<a) max=a;
    if (min>a) min=a;
  }
  sum = sum - max - min;
  ans = sum/(num-2);
  printf("%.2f",ans);
  return 0;
} 
